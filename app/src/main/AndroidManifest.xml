<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="apipet.web.apipet">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icono_dos"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icono_dos"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainScreen" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".MascotasScreen" />
        <activity android:name=".InicioSesion" />
        <activity android:name=".ConsidScreen"></activity>
    </application>

</manifest>